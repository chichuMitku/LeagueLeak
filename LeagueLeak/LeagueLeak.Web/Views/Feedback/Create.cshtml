@model LeagueLeak.Web.Models.Feedbacks.FeedbackViewModel
@{
    ViewBag.Title = "Add Guide";
}

<div class="jumbotron" style="width: 50%">
    @using (Html.BeginForm("Create", "Feedback", FormMethod.Post, new { @class = "form", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()

        <fieldset>
            <legend>Give feedback</legend>
            @Html.EditorFor(m => m.Title)
            @Html.EditorFor(m => m.Content)
            <div class="col-md-offset-2">
                <input type="submit" class="btn btn-default" />
            </div>
        </fieldset>
    }
</div>
@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
}