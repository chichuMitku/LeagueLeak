﻿@model string
<div class="row">
    <div class="col-md-2">
        @Html.LabelFor(m => m)
    </div>
    <div class="col-md-7">
        @Html.TextBoxFor(m => m, new { @class = "form-control" })
    </div>
    <div class="text-danger col-md-3">
        @Html.ValidationMessageFor(m => m)
    </div>
</div>
<br />